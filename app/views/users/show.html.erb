<p><%= @user.username %></p>
<p><%= @user.email %></p>
<p><%= @user.date_of_birth %></p>

<% if user_priviliged?(current_user, @user) %>
  <p><%= link_to 'Delete account', user_path(@user), method: :delete, confirm: 'Are you sure you want to delete your account?' %></p>

  <p><%= link_to 'Edit account', edit_user_path(@user)%></p>
<% end %>
